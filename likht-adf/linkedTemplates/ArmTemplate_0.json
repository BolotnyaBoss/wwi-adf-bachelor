				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@{item()}.parquet",
							"type": "Expression"
						},
						"folderPath": {
							"value": "data/@{pipeline().parameters.Schema_Name}/@{item()}",
							"type": "Expression"
						},
						"fileSystem": "likht"
					},
					"compressionCodec": "snappy"
				},
				"schema": [
					{
						"name": "CityID",
						"type": "INT32"
					},
					{
						"name": "CityName",
						"type": "UTF8"
					},
					{
						"name": "StateProvinceID",
						"type": "INT32"
					},
					{
						"name": "LocationLat",
